{
  "/api/**": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/oauth2": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/swagger-ui.html": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true
  },
  "/v1/api-docs": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true
  },
  "/actuator": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true
  },
  "/ws": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "ws": true,
    "logLevel": "debug",
    "timeout": 120000,
    "headers": {
      "Connection": "Upgrade",
      "Upgrade": "websocket"
    }
  }
} 